
html,body,div,ul,li,ol,input,textarea,a,img,p,h1,h2,h3,h4,h5,h6,form,dl,dt {
	/*margin: 0;*/
	padding: 0;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

body {
	background: #f5f5f5;

}
html {
	/*font-size: 100px;*/

}
header,section,footer,aside,nav,article,figure {
	display: block;
}
select {	/*将默认的select选择框样式清除*/
	appearance: none;
	-moz-appearance: none;
	-webkit-appearance: none;
}
select::-ms-expand { /*清除ie的默认选择框样式清除，隐藏下拉箭头*/
	display: none;
}

img {
	vertical-align: middle;
	border: 0;
}
button {
	outline-style: none;
	outline: none;
	resize: none;
	-webtkit-appearance: none;
}
input,
textarea {
	outline-style: none;
	outline: none;
	-webkit-appearance: none;
	border: none;
	resize: none;
}
label {
	font-weight: 400;
}
ul,li{
	list-style: none;
}
a {
	text-decoration: none;
}
a:hover {
	cursor: pointer;
	text-decoration: none;
}
img {
	vertical-align: middle;
}
i,b,em{
	font-style: normal;
	font-weight: 400;
}
.clearfix::before,
.clearfix::after {
	content: "";
	clear: both;
	height: 0;
	display: block;
	visibility: hidden;
	overflow: hidden;
}
.clepadding {
	padding-left: 0;
	padding-right: 0;
}

/*公共部分*/
/*公共部分*/

.mycollect_top_a {
	height:0.94rem;
	overflow: hidden;
}

.mycollect_top_a {
	background-color: #52DFAA;
}
.top_a_ul {
	 padding: 0 0.3rem;
	width: 100%;
	height: 100%;
}

.top_a_ul li {
	float: left;
	line-height:0.94rem;
	height: 100%;
	color: #E2F9F0;
}
.top_a_le{
	cursor: pointer;
	width: 12%;
	text-align: left;
}
.top_a_ri {
	cursor: pointer;
	width: 18%;
	text-align: left;
}
.top_a_le img{
	width:0.25rem;
	height:0.42rem;
}
.top_a_ri{
	text-align: right;
	font-size: 0.29rem;
}
.top_a_ri img{
	width:1.8rem;
	height:1.5rem ;
}
.top_a_con {
	text-align: center;
	width: 70%;
	font-size: 0.39rem;
	height: 100%;
}
/*居中公共样式*/
.autoCom{
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%) ;
	-webkit-transform: translate(-50%, -50%) ;
	-moz-transform: translate(-50%, -50%) ;
	-ms-transform: translate(-50%, -50%) ;
	-o-transform: translate(-50%, -50%) ;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.touch_action{
	-webkit-touch-callout:none;  
	-webkit-user-select:none;  
	-khtml-user-select:none; 
	-moz-user-select:none;  
	-ms-user-select:none; 
	user-select:none;  
}
/*弹出背景*/

.ifra {
	display: none;
	position: fixed;
	z-index: 200;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, .4);
}