.header{
  overflow: hidden;
    .meau{
      color:#fff;
      font-weight: 800;
      font-size: 23px;
      position: absolute;
      right: 7%;
      transition: rotate .2s linear;
    }
    .search{
      color:#fff;
      font-weight: 800;
      font-size: 22px;
      position: absolute;
      right: 23%;
    }
}
.hidden{
  position: relative;
  overflow: hidden;
  width: 100%;
}
.meau_box{
  background-color:#fff;
  display: flex;
  flex-wrap: wrap;
  overflow: hidden;
  width: 100%;
  position: absolute;
  top:1.17333333rem;
  right: -100%;
  transition: right .2s linear;
  box-sizing: border-box;
  padding:  .1rem .2rem;
  .meau_item{
    width: 25%;
    padding: .12rem 0;
    text-align: center;
    box-sizing: border-box;
    color:#454545;
  }
}
.bodies{
  display: none;
  color:#3f3f3f;
  font-family: '方正兰亭', sans-serif;
  .nav{
    display: flex;
    align-items: center;
    height: 1.2266667rem;
    .select{
      width: 25%;
      text-align: center;
    }
  }
  .group{
    .icon{
      font-size:25px;
      padding-right: 3px;
      vertical-align: middle;
    }
    >div{
      display: flex;
      height: 1.2266667rem;
      align-items: center;
    }
    .text{
      width: 80%;
    }
    span[class*="btn_"]{
      font-size: 20px;
      color:#a4a4a4;
    }
    span[class*="btn_"]:before{
      display: inline-block;
      vertical-align: middle;
      transform: rotate(180deg);
    }
    .invite{
      border-bottom: 1px solid #cfcfcf;
    }
  }
  .select_main{
    .box{

    }
  }
  .everyone{
    a{
      display: flex;
      height: 1.17333333rem;
      align-items: center;
      justify-content: center;
    }
    .photo{
      width: 0.88rem;
      height: 0.88rem;
      border-radius: 50%;
      background-color: #66afe9;
    }
    .message{
      display: flex;
      width: 80%;
      box-sizing: border-box;
      justify-content: space-between;
      .name{
        margin-left: 0.2666rem;
        color:#494949;
      }
      .time{
        margin-right: 0.666rem;
        color:#787878
      }
    }
    .operation{
      span{
        font-size: 23px;
      }
    }
  }
}
.load{
  position: fixed;
  top:0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}