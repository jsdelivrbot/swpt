.header{
  position: fixed;
  top: 0;
  .quit{
    position: absolute;
    left:5%;
    width: 10%;
    color:#fff;
  }
}
.top{
  position: fixed;
  top:1.17333333rem;
  width: 100%;
  background-color: #fff;
  z-index: 9999;
  padding-bottom: 0.1rem;
  transition: max-height .1s;
  max-height: 2rem;
  .navbar{
    margin-top: .1rem;
    color:#333;
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 18px;
    div{
      text-align: center;
      width: 50%;
    }
  }
  .condition{
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin-top: 0.4rem;
    font-size: 16px;
    color:#666;
    i{
      font-size: 23px;
      vertical-align: middle;
      padding-right: 5px;
    }
  }
}
.bodies{
  padding-top: 3.17333333rem;
  color:#333;
  font-size: 16px;
  .moudle{
    width: 100%;
    overflow: hidden;
    margin-top: 0.4rem;
    .moudleBox {
      width: 200%;
      display: block;
      transition: all .3s;
      >div{
        float: left;
        width: 50%;
        height:auto
      }
    }
    .group{
      padding-bottom: .2rem;
      margin-bottom: .2rem;
      border-bottom: 1px dashed #aaa;
      display: flex;
      .phone{
        border-radius: 50%;
        width: 1.26666666rem;
        height: 1.26666666rem;
        background-color: #66afe9;
      }
      .container{
        flex: 8;
        line-height: 0.633333rem;
        padding-left: 0.2666666666rem;
        .call{
          font-size: 15px;
          text-indent: 10px;
          font-weight: 600;
        }
        .type{
          font-size: 13px;
          color:#555;
        }
        .starttime,.endtime{
          font-size: 13px;
          color: #696969;
        }
      }
      .time{
        flex: 4;
      }
    }
  }
}
.conditionModal{
  position: fixed;
  width: 100%;
  overflow: hidden;
  top: 1.17333333rem;
  box-sizing: border-box;
  padding: 0.11333333rem 0.1933333rem .5rem;
  background-color: rgba(33,33,33,.5);
  z-index: -1;
  transition: all .1s;
  opacity: 0;
  .box{
    box-sizing: border-box;
    transform: translateY(200%);
    padding: 0.11333333rem 0.1933333rem .5rem;
    border-radius:0.16rem;
    transition: all .3s;
    background: #fff;
    .CM_head{
      text-align: center;
    }
    .CM_state,.CM_type{
      margin-top: .4rem;
    }
    .CM_title{
      font-weight: 600;
      font-size: 17px;
    }
    .CM_body{
      display: flex;
      flex-wrap: wrap;
      div{
        text-align: center;
        line-height: 0.93333333rem;
        width: 25%;
        font-size: 16px;
        color:#444;
      }
    }
    .CM_button{
      text-align: center;
      font-size: 19px;
      color:#eeeeee;
      margin:1rem auto;
      width: 8.37333333rem;
      height: 1.066666666rem;
      line-height: 1.066666666rem;
      background-color:#f23a3a;
    }
  }
  i{
    font-size: 24px;
    vertical-align: middle;
    padding-right: 5px;
  }
}
.prevent{
  position: fixed;
  width: 100%;
  overflow: hidden;
  top: 1.17333333rem;
  box-sizing: border-box;
  opacity:0;
  z-index: -1;
}
.loading,.hint{
  display: none;
  text-align: center
}
.load{
  width:100%;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: transparent;
  z-index: -1;
  opacity: 0;
}
.hint{
  margin-top: .3rem;
}