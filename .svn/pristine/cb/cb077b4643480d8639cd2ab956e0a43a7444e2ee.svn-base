/* ===================  COMMENT  =================== */

.phone .comment {
    position: fixed;
    top: 0;
    left: 0;
    background: #fff;
    width: 100%;
    height: 100%;
}

.phone .comment .bottomTool {
    height: 5rem;
    box-sizing: border-box;
    padding: 8px 8px;
    text-align: center;
}

.phone .comment .bottomTool #commentText {
    width: 80%;
    height: 100%;
    border: none;
    text-indent: 1rem;
}

.phone .comment .bottomTool #commentSend {
    border: none;
    padding: 1.2rem;
    color: #fff;
    background: #c1c1c1;
    border-radius: 3px;
    margin-left: 0.3rem;
}

.phone .tpl2 .comment .commentCenter {
    padding-top: 4.5rem;
    padding-bottom: 5rem;
    overflow: hidden;
}

.phone .commentCenter .noComment {
    text-align: center;
    margin-top: 9rem;
    display: block;
}

.phone .commentCenter .noComment i {
    font-size: 5rem;
}

.phone .commentCenter .noComment p {
    font-size: 2rem;
}

.phone .commentCenter .hasComment {
    overflow: hidden;
    overflow-y: scroll;
    display: none;
    padding: 0 10px;
    height: -webkit-fill-available;
    height: fill-available;
}

.phone .commentCenter .hasComment .title {
    width: 100vw;
    font-size: 2rem;
    padding: 0;
    font-weight: bold;
    color: #636363;
    position: relative;
    margin: 10px 10px 0 12px;
}

.phone .commentCenter .hasComment .title p::before {
    content: '';
    width: 6px;
    height: 28px;
    background: #636363;
    display: inline-block;
    position: absolute;
    left: -12px;
}

.phone .commentCenter .commentContent {
    margin-top: 20px;
    padding-bottom: 15px;
    border-bottom: 1px dashed black;
}

.phone .commentCenter .commentContent .left {
    float: left;
}

.phone .commentCenter .commentContent .right {
    padding-left: 70px;
}

.phone .commentCenter .commentContent .userIcon {
    width: 60px;
    height: 60px;
}

.phone .commentCenter .commentContent .userIcon img {
    width: 100%;
    height: 100%;
}

.phone .commentCenter .commentContent .userName {
    color: #2bb9ff;
    display: inline-block;
    width: 60%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.phone .commentCenter .commentContent .commentTime {
    float: right;
}

.phone .commentCenter .commentContent .content {
    word-break: break-word;
}

.phone .commentCenter .commentContent .commentTool {
    text-align: right;
}

.phone .commentCenter .commentContent .commentTool span:first-of-type {
    margin-right: 15px;
}

.ztree {
    float: left;
    overflow-y: auto;
    height: 100vh;
    box-sizing: border-box;
    max-width: 250px;
}

.treeView {
    overflow: hidden;
    height: 100vh;
    box-sizing: border-box;
}

.treeView #treeViewIframe {
    width: 100%;
    height: 100%;
}
