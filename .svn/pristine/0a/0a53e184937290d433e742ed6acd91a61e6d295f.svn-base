<div class="re_content">
    <div style="padding: 10px;">
        <ul class="list-group">
            <li class="list-group-item" ng-repeat="o in searchData">
                <span class="badge badge_suffix">{{o.repositorySuffix}}</span>
                <span class="badge">{{teaching_plan_type[o.id].title}}</span>
                <div><a href="#!/detailed/{{o.id}}">{{o.repositoryName}}</a><br/><span>author：{{o.repositoryAuthor}}</span></div>
            </li>
        </ul>
    </div>
<tm-pagination conf="paginationConf"></tm-pagination>
</div>

