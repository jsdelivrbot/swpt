<div class="search_top">
    <div class="search-re"><span onclick="javascript:history.go(-1);"></span></div>
    <input type="text" ng-model="title" placeholder="请输入搜索内容">
    <div class="search-btn"><span ng-click="search()">搜索</span></div>
</div>
