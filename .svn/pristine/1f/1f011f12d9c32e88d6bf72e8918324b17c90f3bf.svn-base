    padding: 1rem;
    box-shadow: 0px 0px 3px 4px #aaa;
    cursor: pointer;
}

.pulish_nav .tabItem:hover{
    border:1px solid red;
   
}

.pulish_nav .tab_item{
    position: absolute;
    top: 50px;
    width: 2rem;
    height: 2rem;
    right: 15px;
}
.del_lf{
    float: left;
}
.del_rg{
    float: left;
    margin-left: 2rem;
    margin-top:1rem;
}
.pulish_nav  .del_lf img{
    width: 6rem;
    height: 8rem;
}
.pulish_nav img{
    width: 100%;
    height: 100%;
    display: block;
}

.iuess div{
    font-size: 15px;
    margin-bottom:2rem;
}
.tab_state{
    color: #FF9900;
}
 .item_time{
    color: firebrick;
}
.tab_item a{
    display: inline-block;
    border: 1px solid;
    width: 6rem;
    text-align: center;
    border-radius: 3px;
    margin-left: .5rem;
    cursor: pointer;
}
.tab_item a:first-child{
    margin-left: 0;
}
.tab_item a:hover{
    border: 1px solid  red;
}
.pulish_nav .tab_name{
    width: 45%;
