/* 首页 */
.pulish{
    height: 100%;
    width: 100%;
}
.pulish .pulish_new{
    text-align: center;
}
.pulish_new .pulish_item{
    display: inline-block;
    width: 40%;
    height: 3rem;
    line-height: 3rem;
    text-align: center;
    border: 1px solid;
    margin-top: 3rem;
    cursor: pointer;
    font-size: 1.5rem;
}
.pulish_new .pulish_item:hover{
    border: 1px solid #53dfa9;
    color: #53dfa9;
}
.pulish .pulish_nav{
    width: 90%;
    margin: 3rem auto;
}
.pageConf{
    width: 90%;
    margin: 1rem auto;
    text-align: center;
}
.page-list input{
    width: 2rem;
}
.pulish_nav .tabNav li{
    display: inline-block;
    text-align: center;
    width: 24%;
    height: 3rem;
    line-height: 3rem;
    border: 1px solid;
    font-size:1.6rem;
    cursor: pointer;
    border-radius: 0.5rem;
    background: #53dfa9;
    color: #fff;
}
.pulish_nav_app .tabNav li:hover,.pulish_nav_app .tabNav li.active{
  
    box-shadow: 0px 3px 0px rgba(219, 31, 5, 1), 0px 3px 6px rgba(0, 0, 0, .9);
