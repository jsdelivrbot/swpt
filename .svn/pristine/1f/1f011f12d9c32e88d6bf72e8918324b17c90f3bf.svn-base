/*章节内容*/
.pc_left{
    width: 40%;
    height: 100%;
    float: left;
    background-color: #F8F8F8;
    border-right: 1px solid #D9D8D8;
    box-sizing: border-box;
}
.pc_right{
    width: 60%;
    height: 100px;
    float: left;
}
.p_confirm{
    clear:both;
    width: 100%;
    text-align: center;
    height: 46px;
    line-height: 46px;
    border-top:1px solid #D9D8D8;
    background-color: #53dfa9;
    cursor: pointer;
    color: #ffffff;
    position: absolute;
    bottom: 0px;
}
.pc_left{
    overflow-y: auto;
}
.pc_left li{
    height: auto;
    overflow: hidden;
    padding: 10px 10px;
    border-left:8px solid #F8F8F8;
    box-sizing: border-box;
    color: #7D7D7D;
    cursor: pointer;
}
.pc_left li.pc_list_act,.pc_left li:hover{
    border-left:8px solid #53dfa9;
    color: #83D7B7;
}
.pc_right{
    overflow-y: auto;
    height: 100%;
}
.pc_right li{
    height: 45px;
    line-height: 45px;
    border: 1px solid #ffffff;
    border-bottom: 1px solid #EBEBEB;
    text-align: center;
    cursor: pointer;
    color: #666666;
}
.pc_right li.pc_list_act,.pc_right li:hover{
    color: #53dfa9;
    border: 1px solid #72E0BB;
}

/*选择资源*/
