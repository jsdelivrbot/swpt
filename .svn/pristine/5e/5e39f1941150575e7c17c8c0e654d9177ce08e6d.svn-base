<script type="text/javascript">
    var frontData = JSON.parse('{"semester_id":"16","subject_id":"47","subject_name":"\u8ba1\u7b97\u673a\u5e94\u7528\u4e0e\u7f51\u7edc","semester_name":"16\u8ba1\u7528\u73ed(\u4e0a\u518c)","section_id":"741","student_id":"33"}');
    $(function() {
        $('#select_child').change(function(){
            $('#select_form').submit();
        })
        /* 展开对应的章节列表 */
        $('#files').tree({
            expanded: 'li[data-sid="741"]'
        });

        /*学科导航菜单*/
        $('.nav ul>li a').click(function(){
            $('.nav ul li').removeClass('li_line_style');
            $(this).parent().addClass('li_line_style');
            var id = $(this).attr('data-date');
            var text = $('.nav').find('.li_line_style').text();
            if (text == '其他'){
                $('#otherModal').modal('show');
            }
        });

        /*左侧菜单按钮*/
        $('body').delegate('a.children_btn','click',function(){
            var parent=$(this).parents('.node_ul');
            parent.find('.children_btn').removeClass('a_hover');
            $(this).addClass('a_hover');

        });
        $('body').delegate('a.is_click_li_a','click',function() {
            $('.is_click_li').removeClass('is_click_li');
            $(this).parents('li').addClass('is_click_li');
            $('.see_more_paper,.see_more_micro,.see_more_question,.micro,.question,.paper').removeClass('hide');
        })

        /*年级选择*/
        $('body').delegate('ul.grade_class li','click',function(){
            $('.is_click').removeClass('is_click');
            $(this).children('a').addClass('is_click');
        });

        /*关闭年级选择*/
        $('.save_btn').click(function(){
            $('#myModal').modal('hide');
        });

        /*选择其他*/
        $('.save_btn_other').click(function(){
            $('#otherModal').modal('hide');
        });

        /* 点击切换错题集内容 */
        $(document).on('click', '[data-type]', function(event) {
            var s = $(this);
            var type = s.data('type');
            var val = s.data(type);
            frontData[type] = val;
            if (type == 'section') {
                var section_id = s.data('section_id');
                frontData['section_id'] = section_id;
            }

            var url = '/index.php?m=Parent&c=Homework&a=listHomework';
            var param = objToParam(frontData);
            location.href = url + '&' + param;
        });
    });

    /* 对象转url参数 */
    function objToParam(obj) {
        if (typeof obj != 'object') {
            return false;
        }
        var paramArr = [], _str;
        for (var i in obj) {
            _str = i + '=' + encodeURIComponent(obj[i]);
            paramArr.push(_str);
        }
        var paramStr = paramArr.join('&');
        return paramStr;
    }
</script>