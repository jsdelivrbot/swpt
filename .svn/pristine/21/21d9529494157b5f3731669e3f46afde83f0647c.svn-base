.myAddress{
	padding:0 0.25rem;
}
.myAddress ul{
	margin: 0 0 0.86rem 0;	
	font-size: 0.26rem;
	padding-bottom: 1.5rem;
}
.myAddress ul li{
	width: 100%;
	height: 1.8rem;
	padding: 0.15rem 0.15rem;
	background: white;
	margin-top: 0.15rem;
}
.myAddress ul li>div{
	width: 100%;
	height: auto;
	display: block;
}

.myAddress ul li>div:nth-of-type(1) span{	
	width: 50%;
	float: left;
	color: #333;
}
.myAddress ul li>div:nth-of-type(1) span.namesNumber{
	text-align: right;
}
.myAddress ul li>div .close-stybtn{
	display: flex;
	justify-content: center;
	float: left;
}
.close-stybtn input{
	width: 0.38rem;
	height: 0.38rem;
	margin-right: 5px;
	border: 1px solid gainsboro;
	border-radius:100%;
}
.close-stybtn label{
	display: block;
	line-height: 0.5rem;
	color: #666;
}
.addres-btn{
	float: right;
	padding-left: 20px;
}
.addres-btn a{
	color: #666;
	padding: 0 0.11rem;
}
.addres-btn a img{
	width: 0.25rem;
	height: 0.25rem;
	margin-right: 0.11rem;
}
.isInput{
	padding-bottom: 5px;
	border-bottom: 1px solid #979797;
}
.addAddress{
	display: block;
	position: fixed;
	width: 100%;
	background:#52dfaa;
	padding: 0.3rem 0;
	text-align: center;
	color: white;
	font-size: 0.26rem;
	bottom: 0px;
}
.addAddress a{
	color: white;
}
/*编辑地址*/
.address_cont,.address_eadit{
	 z-index:-999;
	background: white;
	margin-top: 0.11rem;
	padding: 0 0.25rem;
}
.address_cont p,.address_eadit p{
	width: 100%;
	padding: 0.3rem 0; 
	font-size:0.26rem;
}
.address_cont p span.down_icon,.address_eadit p span.down_icon{
	display: inline-block;
	width: 1.0rem;
	float: right;
}
.address_cont p span.down_show,.address_eadit p span.down_show{
	display: inline-block;
	margin-left: 1.5rem;
}
.address_cont p input[type=text],.address_eadit p input[type=text]{
	width: 100%;
	padding-left:1.5rem;
}
.address_cont p span.address_title,.address_eadit p span.address_title{
	display:inline-block;
	float: left;
	position: absolute;
}
.address_cont p span.address_title_ssq,.address_eadit p span.address_title_ssq{
	width: 100%;
	display:inline-block;
}
.setMoren{
	background: white;
}
.setMoren input{
	width:0.3rem;
	height: 0.3rem;
	vertical-align: middle;
	border: 1px solid gainsboro;
	border-radius: 0.15rem;
	
}
.setMoren label{
	color: #999999;
}
