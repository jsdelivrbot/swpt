/*弹出框*/
.chooseCity{
	width: 80%;
	margin:0 auto;
	background: white;
	position: absolute;
	left: 50%;
	top:50%;
	transform: translateX(-50%) translateY(-50%);
	-webkit-transform: translateX(-50%) translateY(-50%);	
	-moz-transform: translateX(-50%) translateY(-50%);
	-ms-transform: translateX(-50%) translateY(-50%);
	-o-transform: translateX(-50%) translateY(-50%);
}
.chooseCity .box{
	height: 400px;
	position: relative;
    overflow-y: scroll;
}
.chooseCity p{
	padding: 10px 0;
	background: #f5f5f5;
	color:#69e2b3 ;
	text-align: center;
}
.chooseCity ul{
	width: 100%;
	overflow-y: scroll;
    float: right;
    overflow-x: hidden;
}
.chooseCity ul li{
	text-align: left;
	padding: 10px;
	border-bottom: 1px solid #D3D3D3;
}
.chooseCity .aboulBtn{
	width: 100%;
	z-index: 999;
	text-align: center;
}
.chooseCity .aboulBtn a{	
	width: 50%;
	display: block;
	float: left;
	line-height: 40px;
	text-align: center;
	background: #f5f5f5;
	color: #666;
}
.chooseCity .aboulBtn a:first-child{
	border-right: 1px solid #D3D3D3;
}
/*插件的城市列*/
 .select-city {
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 999999;
}

.select-city-shade {
	position: absolute;
	width: 100%;
	height: 100%;
	z-index: 1;
	background: #000000;
	opacity: 0.7;
}

.select-city-container {
	position: absolute;
	width: 200px;
	height: 320px;
	top: 50%;
	left: 50%;
	margin-top: -170px;
	margin-left: -110px;
	background: #ffffff;
	z-index: 100;
	border-radius: 8px;
	padding-left: 10px;
	padding-right: 10px;
}

.select-city-title {
	position: absolute;
	width: 94%;
	height: 39px;
	line-height: 50px;
	left: 3%;
	top: 0;
	border-bottom: 1px #c2c2c2 solid;
	font-weight: 700;
	z-index: 99999;
}

.selected-prov {
	position: relative;
	max-width: 30%;
	height: 40px;
	/*float: left;*/
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-webkit-text-overflow: ellipsis;
	color: #D8AC62;
	font-size: 14px;
	padding-right: 5px;
}

.selected-city {
	position: relative;
	max-width: 30%;
	height: 40px;
	/*float: left;*/
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-webkit-text-overflow: ellipsis;
	color: #D8AC62;
	font-size: 14px;
	padding-left: 5px;
	padding-right: 5px;
	display: none;
}

.selected-dist {
	position: relative;
	max-width: 30%;
	height: 40px;
	/*float: left;*/
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-webkit-text-overflow: ellipsis;
	color: #D8AC62;
	font-size: 14px;
	padding-left: 5px;
	padding-right: 5px;
	display: none;
}

.select-close {
	position: absolute;
	width: 40px;
	height: 40px;
	top: -3px;
	right: 25px;
	line-height: 50px;
	text-align: right;
	font-size: 22px;
}

.select-item-container {
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	box-sizing: border-box;
	border-top: 40px solid transparent;
	overflow: auto;
	-webkit-overflow-scrolling: touch;
	-webkit-text-size-adjust: none;
	font-size: 14px;
}

.prov-item-container,
.city-item-container,
.dist-item-container {
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
}

.select-item {
	/*position: relative;*/
	width: 94%;
	height: 39px;
	border-bottom: 1px #C2C2C2 solid;
	margin-left: 3%;
	line-height: 40px;
}

.select-item span {
	position: absolute;
	width: auto;
	height: 50%;
	right: 0;
	line-height: 40px;
}
