@charset "UTF-8";

.i_jczw .tree-light li,.i_jscy .tree-light li{
    height: 31px;
    line-height: 31px;
}
.i_jczw .tree-light .tree-selected,.i_jscy .tree-light .tree-selected{
    font-weight: 400;
    background: rgba(0,168,255,.7);
    color:#fff;
}
.m_title{
    text-indent: 30px;
    font-size: 16px;
    padding-top:5px;
    padding-bottom: 5px;
    background: #367fa9;
    color: #fff;
    font-weight: 400;
}
.m_title .c_left,.m_title .c_right{
    text-indent: 15px;
    text-align: left;
}
.i_jczw .tree-light li{}
.i_jczw  .tree-label,.i_jscy .tree-label{
    display: block;
    position: absolute;
    top: 0;
    left:0;
    letter-spacing: 3px;
    text-indent: 30px;
    width: 100%;
    font-size:16px;
    color:#707070
}
.group{
    width: 100%;
    margin-left:10px ;
}
.group .g_item:hover{
    background-color: #337ab7;
    color:#fff;
}
.user_item{
    overflow: hidden;
}
.c_left{
    float: left;
}
.user_item .c_left{
    height: 30px;
    border-bottom: 1px solid #dfdfdf;
}
.c_right{
    border-bottom: 1px solid #dfdfdf;
}
.user_item .c_right{
    height: 30px;
    float: right;
}
.c_left,.c_right{
    width: 50%;
    padding-left: 5px;
    padding-right: 5px;
    font-size: 15px;
    text-align: center;
    letter-spacing: 1px;
    text-indent: 0;
}
.i_jscy li{
    padding: 0;
    list-style: none;
}
.i_jscy li:nth-child(even){
    background:#f9f9f9
}
.i_jscy .tree-label:hover{
    background: #e7e7e7;
}

treecontrol ul{
    padding-bottom: 10px;
}
#aside ul{
    padding-bottom: 0;
    transition: all .3s;
}
#aside li{
    padding: 3px 0 0 20px;
    transition: all .3s;
}
#aside .tree-selected{
    padding: 3px 5px;
    background: #d9e8fb;
}
.mt_group{
    overflow: hidden;
    background: #f5f5f5 url("../images/gridbar.jpg");

}
.mt_ul input{
    width: 15px;
    height: 15px;
    margin-left: 10px ;
    margin-top: 6px;
}
.modal-body{
    padding-top: 0;
    padding-bottom: 20px;
}
.mt_btn{
    float: left;
    height:28px;
    text-align: center;
    cursor: pointer;
    line-height: 28px;
    font-size: 15px;
    padding-left: 8px;
    padding-right: 8px;
}
.mt_btn:hover{
    background-color: #337ab7;
    color:#fff;
}
.mt_title{
    overflow: hidden;
    width: 100%;
    background: #367fa9;
    color: #fff;
}
.mt_title .mt_item{
    width: 15%;
    text-align: center;
    font-size: 15px;
    height: 25px;
    line-height: 25px;
}
.mt_title div{
    float: left;
}
.MT_Item{
    overflow: hidden;
}
.MT_div{
    float: left;
    width: 15%;
    text-align: center;
    font-size: 15px;
    height: 25px;
    line-height: 25px;
}
.pk_item{
    float: left;
}
.i_ul{
  padding-left: 0 !important;
  height: 250px;
}
.page-list{
    width: 100%;
    overflow: hidden;
}
.pagination{
    width: 40%;
    float: left;
}
.page-total {
    width: 51%;
    float: right;
    margin-top: 22px;
    font-size: 14px;
}
.mt_ul .ng-pristine{
    width: 25px;
    height:20px;
}
#addNewU .col-sm-9{
    margin-bottom: 5px;
}
#addNewU .control-label{
    margin-top: 7px;
}
#datamodal{
    width: 100%;
    height: 100%;
    background: rgba(55,55,55,.5);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    display: none;
}
#databox{
    width: 80%;
    height:400px;
    position: absolute;
    overflow-y: scroll;
    top:10%;
    left: 10%;
    z-index: 9999;
    display: none;
    background-color: #fff;
    transition: all 0.3s;
    border-radius: 10px;
}
.data_header{
    width:100%;
    font-size: 18px;
    letter-spacing: 1px;
    padding-left: 50px;
    height: 50px;
    line-height: 50px;
    color:#5f5f5f;
    border-bottom: 1px solid #d1dae1;
}
.ultoggle{
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgb(189,189,189);
    padding: 5px;
    border-radius: 6px;
}
#aside .ultoggle li{
    padding: 5px;
}
.data_main {
    width: 70%;
    margin: 20px auto 0;
}
.data_main .checkbox{
    font-size: 18px;
}
.data_main .d_label{
    position: absolute;
    left:0px;
    width: 20%;
    height: 34px;
    line-height: 34px;
    font-size: 18px;
    text-align: center;
}
.data_btn{
    margin-top: 30%;
    padding: 0 45%;
}
.datap{
    width: 80%;
    margin-left: 20%;
}
/*用户时间插件*/


/*用户管理模态框*/
@media (min-width: 768px){
    .modal-dialog {
        width: 75%;
        margin: 30px auto;
    }
}