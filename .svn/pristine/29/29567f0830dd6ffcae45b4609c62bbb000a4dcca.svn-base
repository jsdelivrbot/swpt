.listtype{
  padding: 10px;
}
.listtype dl{
  float: left;
  width: 200px;
  height: 157px;
  border: 1px solid #ccc;
  border-radius: 5px;
  margin-right: 5px;
}
.listtype dt{
  text-align: center;
  height: 130px;
}
.listtype dt img{
  width: 96%;
}
.listtype dd{
  height: 27px;
  line-height: 27px;
}
