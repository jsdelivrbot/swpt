.header {
  height: 1.17333333rem;
  width: 100%;
  align-items: center;
  line-height:1.17333333rem;
  position: relative;
  color:#fff;
  font-size: 18px ;
  font-weight: 500;
  text-align: center;
  background-color:#f23a3a;
  .quit{
    position: absolute;
    left:5%;
    width: 10%;
  }
}
.bodies{
  font-size: 15px;
  .phone{
    width: 2rem;
    height: 2rem;
    background-color: #66afe9;
    margin:1.0666666rem  auto 0;
  }
  .name{
    margin-top: .4rem;
    text-align: center;
  }
  .applydata,.applystate,.applytime,.applydeed{
    width: 90%;
    margin: 0 auto;
    text-indent:0.266666rem;
  }
  .applydata,.applystate,.applytime{
    height: 1.52rem;
    line-height: 1.52rem;
    border-bottom: 1px solid #eee;
  }
  .variabledata,.variabletime,.variablestate{
    padding-left: 0.53333rem;
    font-size: 17px;
  }
  .variabledata{
    color:#169bd5;
  }
  .deedtitle{
    height: 1.52rem;
    line-height: 1.52rem;
  }
  .deedmain{
    padding:0.53333rem;
    li:last-child .listborder{
      top:0;
      height: 0.7rem;
    }
    li:first-child .listborder{
      top: 0.7rem;
      height: 0.7rem;
    }
    .deedlist{
      display: flex;
      height:1.4rem;
      position: relative;
      line-height: 1.4rem;
      align-items: center;
      .listborder{
        position: absolute;
        left: 5px;
        width:1px;
        z-index: -1;
        height: 1.4rem;
        border-left:1px solid #e3e3e3;
      }
      .listdot{
        width: 11px;
        height: 11px;
        border-radius: 50%;
        background: #3c0;
      }
      .listtext{
        letter-spacing: 1px;
      }
    }
  }
}