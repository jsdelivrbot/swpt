    /*color: #fff;*/
    /*background-color: #337ab7;*/
    /*border-color: #2e6da4;*/
/*}*/
.benchpage .lastpage,.benchpage .nextpage{
    border-left: 1px solid #eee;
}
.benchpage div:nth-child(2){
    border-left: 1px solid #eee;;
}
.benchpage .btn-primary{
    color:#fff !important;
}
.nonelistpage{
    margin: 20px 0;
    text-align: center;
    font-size: 16px;
}
