.open-dig{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.6);
}
.open-content{
    width: 70%;
    height: 80%;
    margin: 30px auto;
    background-color: #ffffff;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius:6px;
    padding: 10px;
    overflow: hidden;
}
.open-head{
    height: 30px;
    line-height: 30px;
    background-color: #f5f5f5;
    padding: 3px;
}
.open-con{
    width: 100%;
    height: 93%;
    overflow-y: auto;
    clear: both;
}
.vip_module,.other_module{
    width: 100%;
    clear: both;
}
.functional_module{
    float: left;
    width: 80px;
    height: auto;
    margin-top: 5px;
    overflow: hidden;
}
.vip_module h2,.other_module h2{
    font-size: 14px;
    height: 14px;
    line-height: 14px;
    border-left: 5px solid #666666;
    padding-left: 5px;
    margin-left: 3px;
    margin-top: 5px;
}
.vip_module_con{
    padding: 10px;
    clear: both;
}
.vip_module h2{
    margin-top: 10px;
}
.vip_module_con h3{
    font-size: 12px;
    padding: 3px;
    background-color: #f5f5f5;
}
.functional_module dt{
    height: 40px;
    text-align: center;
    cursor: pointer;
}
.functional_module dt img{
    width: 40px;
    height: 40px;
}
.functional_module dd{
    text-align: center;
    font-size: 12px;
    height: 30px;
    line-height: 30px;
}
.other_module dd a{
    display: inline-block;
    height: 16px;
    line-height: 16px;
    padding: 1px 3px;
    background-color: #31b0d5;
    text-align: center;
    color: #ffffff;
    margin: 1px;
    vertical-align: middle;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius:4px;
    font-size: 12px;
    margin-top: -20px;
}
.other_module dd a:nth-of-type(2){
    background-color: #d9534f;
}
.other_module{
    height: auto;
    overflow: hidden;
    border-top: 1px dashed #e0e0e0;
}
.open-bot{
    padding: 20px;
    text-align: left;
    clear: both;
}
.open-bot-sub,.open-bot-cls{
    display: inline-block;
    padding: 1px 10px;
    background-color: #337ab7;
    border: 1px solid #2e6da4;
    color: #ffffff;
    font-size: 14px;
    vertical-align: middle;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius:4px;
    cursor: pointer;
}
.open-bot-cls{
    background-color: #bac5ce;
    border: 1px solid #bcc4cc;
}
.open-top-cls{
    display: inline-block;
    width: 18px;
    height: 18px;
    line-height: 18px;
    font-size: 14px;
    background-color: #ff2222;
    float: right;
    margin-right: 3px;
    -moz-border-radius: 9px;
    -webkit-border-radius: 9px;
    border-radius:9px;
    color: #ffffff;
    text-align: center;
    font-weight: 600;
    margin-top: 6px;
    cursor: pointer;
}
.open-top-cls:hover{
    background-color: #00a0e9;
}
/*所以模块列表*/
.module-class-up{
    display: inline-block;
    width:0;
    height:0;
    border-width:15px;
    border-style:solid;
    cursor: pointer;
}
.mfnt1{
    background-color: #00a0e9;
    color: #ffffff;
}
.mfnt1 .module-class-up{
    border-color:red transparent transparent transparent;
}
.mfnt2{
    background-color: #2def43;
    color: #ffffff;
}
.mfnt2 .module-class-up{
    border-color:#f1f907 transparent transparent transparent;
}
.mfnt3{
    background-color: #d8cdd8;
    color: #ffffff;
}
.mfnt3 .module-class-up{
    border-color:#464808 transparent transparent transparent;
}
.module-block-list{
    position: relative;
    width: 100%;
    height: auto;
    overflow: hidden;
    border-bottom: 1px solid #cccccc;
    clear: both;
}
.module-class{
    position: absolute;
    width: 31px;
    height: 100%;
    text-align: center;
    vertical-align: middle;
    border-right: 1px solid #cccccc;
    box-sizing: border-box;
}
.module-class-con{

}
.module-class-lists{
    width: auto;
    height: auto;
    float: left;
    margin-left: 32px;
}
.module-class-lists dl{
    width: 100px;
    height: 106px;
    padding: 10px;
    box-sizing: border-box;
    text-align: center;
    float: left;
    overflow: hidden;
    cursor: pointer;
}
.module-class-lists dl:hover{
    background-color: #f7f7f7;
}
.module-class-lists dt{
    height: 60px;
}
.module-class-lists dt img{
    width: 60px;
    height: 60px;
}
.module-class-lists dd{
    width: 100%;
    height: 26px;
    line-height: 26px;
    font-size: 12px;
    color: #666666;
}
.clear{
    clear: both;
}
.h-nav-ul{
    width: 100%;
    display: flex;
}
.h-nav-li{
    flex: 1;
    text-align: center;
    font-size: 18px;
    padding: 10px 0;
    background-color: #00a0e9;
    color:#fff;
    cursor: pointer;
}
.active_nav_li{
    background-color: #248dde;
}
.h-nav-btn{
    width: 50%;
    display: flex;
    margin: 0 auto;
}
.h-nav-list{
    flex:1;
    text-align: center;
    font-size: 18px;
    cursor: pointer;
    padding: 10px 0;
}
.active_nav{
    position: relative;
}
.active_nav:before{
    position: absolute;
    content: "";
    display: block;
    width: 90%;
    height:4px;
    bottom:-5px;
    left:50%;
    margin-left: -45%;
    background: #000;
}
